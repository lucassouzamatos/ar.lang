fun start () ->
  let pair = fun (X Y) ->
    fun (F) ->
      let Bind = apply F X done
      apply Bind Y done
    done
  done

  let first = fun (X) ->
    fun (Y) ->
      X done
    done
  done

  let second = fun (X) ->
    fun (Y) ->
      Y done
    done
  done

  let completed = apply pair 1 7 done

  apply completed first done
  apply completed second done
done
